<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_task_type_edit_stage_timesheet" model="ir.ui.view">
        <field name="name">project.task.type.checklist.form.stage.timesheet</field>
        <field name="model">project.task.type</field>
        <field name="inherit_id" ref="project.task_type_edit"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='mail_template_id']" position="after">
                <field name="timesheet_mandatory" />
                <field name="timesheet_quantity"/>
            </xpath>
        </field>
    </record>


</odoo>
